<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="1010dp"
    android:layout_height="64dp"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="44.7dp"
        android:id="@+id/id_conmana_lvlayout"
        android:orientation="horizontal"
        android:background="#fff">
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:button="@drawable/gender_checkbox_style"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center"
            android:id="@+id/id_instrulv_choose"/>
        <TextView
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:text="序号"
            android:textAlignment="center"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center"
            android:id="@+id/id_instrulv_pro"/>
        <TextView
            android:layout_width="270.7dp"
            android:layout_height="wrap_content"
            android:text="质控时间"
            android:textAlignment="center"
            android:layout_marginLeft="4dp"
            android:layout_gravity="center"
            android:id="@+id/id_instrulv_time"/>
        <TextView
            android:layout_width="74dp"
            android:layout_height="wrap_content"
            android:text="质控结果"
            android:textAlignment="center"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="20dp"
            android:layout_gravity="center"
            android:id="@+id/id_instrulv_result"/>
    </LinearLayout>
</LinearLayout>
