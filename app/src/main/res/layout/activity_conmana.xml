<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f7fa"
    tools:context="com.example.chen.ls4000.activity.ConmanaActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/bg_headofpage"
        android:id="@+id/id_conmana_title">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string129"
            style="@style/titlefont"
            android:layout_centerInParent="true"
            android:layout_margin="30dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_back"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:id="@+id/id_conmana_back"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:id="@+id/id_conmana_btnlayout"
        android:elevation="10dp">
        <Button
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#fff"
            android:drawableTop="@mipmap/ic_starttest"
            android:id="@+id/id_conmana_startdeter"
            android:text="@string/string130"
            android:textAllCaps="false"
            style="@style/querybtnfont"
            android:paddingTop="16.6dp"/>
        <Button
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:background="#fff"
            android:drawableTop="@mipmap/ic_delete"
            android:id="@+id/id_conmana_delete"
            android:text="@string/string131"
            android:textAllCaps="false"
            style="@style/querybtnfont"
            android:paddingTop="16.6dp"
            android:visibility="gone"/>
        <Button
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:background="#fff"
            android:drawableTop="@mipmap/ic_clear"
            android:id="@+id/id_conmana_clear"
            android:text="@string/string132"
            android:textAllCaps="false"
            style="@style/querybtnfont"
            android:paddingTop="16.6dp"
            android:visibility="gone"/>
        <Button
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#fff"
            android:layout_weight="1"
            android:drawableTop="@mipmap/ic_print"
            android:id="@+id/id_conmana_print"
            android:text="@string/string133"
            android:textAllCaps="false"
            style="@style/querybtnfont"
            android:paddingTop="16.6dp"
            android:visibility="gone"/>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_conmana_title"
        android:layout_above="@id/id_conmana_btnlayout"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:background="#fff"
        android:layout_marginBottom="1dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string207"
            style="@style/settingstextfont"
            android:id="@+id/id_conmana_pretitle"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"/>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#eee"
            android:layout_below="@id/id_conmana_pretitle"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/id_conmana_pretitle"
            android:id="@+id/id_conmana_layout1"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string208"
                style="@style/settingstextfont"/>
            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/id_conmana_pretime"
                android:text=""
                android:enabled="false"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/id_conmana_layout1"
            android:id="@+id/id_conmana_layout2"
            android:layout_alignRight="@id/id_conmana_layout1">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string209"
                style="@style/settingstextfont"/>
            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/id_conmana_preresult"
                android:enabled="false"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/id_conmana_layout2"
            android:id="@+id/id_conmana_layout3"
            android:layout_marginBottom="10dp"
            android:layout_alignRight="@id/id_conmana_layout2">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string210"
                style="@style/settingstextfont"/>
            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/id_conmana_precon"
                android:enabled="false"/>
        </LinearLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#eee"
            android:layout_below="@id/id_conmana_layout3"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string211"
            style="@style/settingstextfont"
            android:id="@+id/id_conmana_curtitle"
            android:layout_below="@id/id_conmana_layout3"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"/>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#eee"
            android:layout_below="@id/id_conmana_curtitle"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/id_conmana_curtitle"
            android:id="@+id/id_conmana_layout4"
            android:layout_marginTop="10dp"
            android:layout_alignLeft="@id/id_conmana_curtitle">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string212"
                style="@style/settingstextfont"/>
            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/id_conmana_curtime"
                android:enabled="false"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/id_conmana_layout5"
            android:layout_below="@id/id_conmana_layout4"
            android:layout_alignRight="@id/id_conmana_layout4">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string213"
                style="@style/settingstextfont"/>
            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/id_conmana_curresult"
                android:enabled="false"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/id_conmana_layout5"
            android:layout_alignRight="@id/id_conmana_layout5"
            android:id="@+id/id_conmana_layout6"
            android:layout_marginBottom="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string214"
                style="@style/settingstextfont"/>
            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/id_conmana_curcon"
                android:enabled="false"/>
        </LinearLayout>
        <!--<com.example.chen.ls4000.utils.CurveChart-->
            <!--android:id="@+id/id_result_customCurveChart"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:orientation="vertical"-->
            <!--android:paddingBottom="10dp"-->
            <!--android:layout_marginRight="20dp"-->
            <!--android:layout_marginTop="10dp"-->
            <!--android:paddingTop="10dp"-->
            <!--android:layout_below="@id/id_conmana_layout6"/>-->
        <!--<com.example.chen.ls4000.utils.CurveChart-->
            <!--android:id="@+id/id_conmana_curvechart"-->
            <!--android:layout_width="600dp"-->
            <!--android:layout_height="260dp"-->
            <!--android:paddingBottom="10dp"-->
            <!--android:layout_marginTop="20dp"-->
            <!--android:layout_marginRight="20dp"-->
            <!--android:layout_below="@id/id_conmana_layout6"-->
            <!--android:visibility="gone"/>-->
    </RelativeLayout>
</RelativeLayout>
